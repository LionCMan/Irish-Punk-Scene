<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.irishpunkscene">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/appNameSpaces"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecentActivity"
            android:label="@string/recent"/>
        <activity android:name=".FavoriteActivity"
            android:label="@string/favorite"/>
        <activity android:name=".RandomActivity"
            android:label="@string/random"/>
        <activity android:name=".BandsActivity"
            android:label="@string/bands"/>
        <activity android:name=".AlbumsActivity"
            android:label="@string/albums">
        </activity>
    </application>

</manifest>